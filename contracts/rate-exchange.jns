(service
  "Rate Exchange"

  (contract "Get currency exchange rate"
    (method :get)
    (url "http://rate-exchange.appspot.com/currency?from=USD&to=INR")

    (should-have :path "$.to" :equal-to "INR")
    (should-have :path "$.frm" :equal-to "USD")
    (should-have :path "$.rate" :of-type :number)
  )
)
